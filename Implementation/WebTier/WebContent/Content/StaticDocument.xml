<?xml version="1.0" encoding="UTF-8"?>
<sd:smartDocumentDefinition 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
   xmlns:pp="http://www.processpuzzle.com" 
   xmlns:sd="http://www.processpuzzle.com/SmartDocument" 
   xsi:schemaLocation="http://www.processpuzzle.com/SmartDocument http://processpuzzle.com/Schemas/SmartDocument.xsd">
   
   <sd:name>Static Document</sd:name>
   <sd:description>Demonstrates static content of a Smart Document.</sd:description>
   <sd:version>0.5</sd:version>
   
   <sd:resources>
      <sd:styleSheets></sd:styleSheets>
      <sd:images></sd:images>
      <sd:javaScripts></sd:javaScripts>
   </sd:resources>

   <sd:documentHeader tag="div" id="staticDocumentHeader">
      <sd:compositeElement tag="div" id="headerWrapper">
         <sd:element tag="h1">StaticDocument.title</sd:element>
         <sd:element tag="h2">StaticDocument.subTitle</sd:element>
      </sd:compositeElement>
   </sd:documentHeader>
   
   <sd:documentBody tag="div" id="smartDocumentBody">
      <sd:element tag="p">StaticDocument.introduction</sd:element>
      <sd:element tag="p">StaticDocument.exampleDescription</sd:element>
   </sd:documentBody>
   
   <sd:documentFooter tag="div" id="staticDocumentFooter">
      <sd:compositeElement tag="div" id="footerWrapper">
         <sd:element tag="h3">StaticDocument.footer</sd:element>
      </sd:compositeElement>
   </sd:documentFooter>
</sd:smartDocumentDefinition>
