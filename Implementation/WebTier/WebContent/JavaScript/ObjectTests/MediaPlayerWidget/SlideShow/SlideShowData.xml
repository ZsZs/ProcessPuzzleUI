<?xml version="1.0" encoding="UTF-8"?>
<sh:slideShow 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns="http://www.w3.org/1999/xhtml"
   xmlns:sh="http://www.processpuzzle.com/SlideShow" 
   xsi:schemaLocation="http://www.processpuzzle.com/SlideShow http://processpuzzle.com/Schemas/SlideShow.xsd">
   
   <sh:name>Pura Vida Costa Rica</sh:name>
   <sh:description>Photos from Costa Rica.</sh:description>
   
   <sh:properties>
      <sh:accessKeys>shift+h</sh:accessKeys>
      <sh:automaticallyLinkSlideToFullSizedImage>true</sh:automaticallyLinkSlideToFullSizedImage>
      <sh:centerImages>true</sh:centerImages>
      <sh:effectDuration>1500</sh:effectDuration>
      <sh:firstSlide>7</sh:firstSlide>
      <sh:galeryLink>http://www.tourism-costarica.com</sh:galeryLink>
      <sh:height>300</sh:height>
      <sh:imageFolderUri>../MediaPlayerWidget/Album</sh:imageFolderUri>
      <sh:loopShow>true</sh:loopShow>
      <sh:overlapImages>true</sh:overlapImages>
      <sh:resizeImages>true</sh:resizeImages>
      <sh:showController>true</sh:showController>
      <sh:showImageCaptions>true</sh:showImageCaptions>
      <sh:showSlidesRandom>true</sh:showSlidesRandom>
      <sh:showThumbnails>true</sh:showThumbnails>
      <sh:skipTransition>SlideShow.WhenPaused | SlideShow.OnStart</sh:skipTransition>
      <sh:slideChangeDelay>3000</sh:slideChangeDelay>
      <sh:slideTransition>elastic:in:out</sh:slideTransition>
      <sh:startPaused>true</sh:startPaused>
      <sh:thumbnailFileNameRule>[/(\.[^\.]+)$/, '_thumb$1']</sh:thumbnailFileNameRule>
      <sh:width>450</sh:width>
   </sh:properties>
   
   <sh:images>
      <sh:image>
         <sh:uri>IMAG0337.jpg</sh:uri>
         <sh:caption>PhotoGalery.tabacon</sh:caption>
         <sh:link>http://www.tabacon.com/</sh:link>
         <sh:thumbnailUri>IMAG0337_thumb.jpg</sh:thumbnailUri>
      </sh:image>
      <sh:image>
         <sh:uri>IMAG0339.jpg</sh:uri>
         <sh:caption>PhotoGalery.baldi</sh:caption>
         <sh:link>http://www.arenal.net/hotel/baldi-hotel/index.html</sh:link>
         <sh:thumbnailUri>IMAG0339_thumb.jpg</sh:thumbnailUri>
      </sh:image>
      <sh:image>
         <sh:uri>SANY0008.JPG</sh:uri>
         <sh:thumbnailUri>SANY0008_thumb.jpg</sh:thumbnailUri>
      </sh:image>
      <sh:image>
         <sh:uri>SANY0012.JPG</sh:uri>
         <sh:thumbnailUri>SANY0012_thumb.jpg</sh:thumbnailUri>
      </sh:image>
      <sh:image>
         <sh:uri>SANY0022.JPG</sh:uri>
         <sh:thumbnailUri>SANY0022_thumb.jpg</sh:thumbnailUri>
      </sh:image>
      <sh:image>
         <sh:uri>SANY0042.JPG</sh:uri>
         <sh:thumbnailUri>SANY0042_thumb.jpg</sh:thumbnailUri>
      </sh:image>
      <sh:image>
         <sh:uri>SANY0058.JPG</sh:uri>
         <sh:thumbnailUri>SANY0058_thumb.jpg</sh:thumbnailUri>
      </sh:image>
      <sh:image>
         <sh:uri>SANY0071.JPG</sh:uri>
         <sh:thumbnailUri>SANY0071_thumb.jpg</sh:thumbnailUri>
      </sh:image>
   </sh:images>
</sh:slideShow>
