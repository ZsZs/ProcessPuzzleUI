<?xml version="1.0" encoding="UTF-8"?>
<sd:smartDocumentDefinition 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
   xmlns:pp="http://www.processpuzzle.com" 
   xmlns:sd="http://www.processpuzzle.com/SmartDocument" 
   xsi:schemaLocation="http://www.processpuzzle.com/SmartDocument http://processpuzzle.com/Schemas/SmartDocument.xsd">
   <sd:name>Desktop Footer Document</sd:name>
   <sd:description>Footer part of MochaUI desktop.</sd:description>
   <sd:version>0.5</sd:version>
   
   <sd:resources>
      <sd:styleSheets></sd:styleSheets>
      <sd:images></sd:images>
      <sd:javaScripts></sd:javaScripts>
   </sd:resources>

   <sd:documentBody tag="div" id="desktopFooterWrapper">
      <sd:compositeElement tag="div" id="desktopFooter">
         <sd:element tag="div" id="footerText" elementStyle="footerText" href="http://processpuzzle.com">DesktopConfigurator.footerText</sd:element>
         <sd:element tag="div" id="SkinSelectorWidget">
            <sd:plugin>
               <sd:resources>
                  <sd:styleSheets>
                     <sd:styleSheet>Desktops/ProcessPuzzle/Styles/SkinSelectorWidget.css</sd:styleSheet>
                  </sd:styleSheets>
               </sd:resources>
               <sd:widget name="SkinSelectorWidget">
                  <sd:options>
                     <sd:option name="widgetContainerId" value="'SkinSelectorWidget'"/>
                  </sd:options>
               </sd:widget>
            </sd:plugin>
         </sd:element>
      </sd:compositeElement>
   </sd:documentBody>
</sd:smartDocumentDefinition>
