<?xml version="1.0" encoding="UTF-8"?>
<pp:tabWidgetDefinition
	xmlns:pp="http://www.processpuzzle.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<tabWidget tabWidgetId="ConsoleTabs" selectedTabClass="selectedTab" showCloseButton="false" showPrintButton="false">
		<tab tabId="ConsoleTab.properties" caption="ConsoleTabs.properties">
         <messageProperties>{ actionType : 'loadDocument', documentURI : 'Content/DocumentPropertiesDefinition.xml', documentContentURI : 'Content/DocumentProperties', documentType : SmartDocument.Types.SMART }</messageProperties>         
      </tab>
		<tab tabId="ConsoleTab.comments" caption="ConsoleTabs.comments" isDefault="true">
         <messageProperties>{ actionType : 'loadDocument', documentURI : 'Content/DocumentCommentsDefinition.xml', documentType : SmartDocument.Types.SMART }</messageProperties>         
      </tab>
		<tab tabId="ConsoleTab.todo" caption="ConsoleTabs.todo">
         <messageProperties>{ actionType : 'loadDocument', documentURI : 'Content/PersonalToDoListDefinition.xml', documentType : SmartDocument.Types.SMART }</messageProperties>         
      </tab>
	</tabWidget>
	
</pp:tabWidgetDefinition>